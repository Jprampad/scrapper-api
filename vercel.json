{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "GOOGLE_CREDENTIALS_JSON": "@google-credentials-json",
    "GOOGLE_SPREADSHEET_ID": "@google-spreadsheet-id"
  }
} 