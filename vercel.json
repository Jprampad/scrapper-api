{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": [
          "docs/endpoints/*.json",
          "routers/**/*",
          "models/**/*",
          "services/**/*",
          "core/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "VERCEL_FORCE_NO_BUILD_CACHE": "1",
    "VERCEL_FORCE_NO_SSO": "1",
    "VERCEL_FORCE_DISABLE_AUTH": "1"
    }
} 